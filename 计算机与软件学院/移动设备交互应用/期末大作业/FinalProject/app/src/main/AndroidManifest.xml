<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalProject">
        <activity android:name=".Main.SearchBoxActivity"
            android:launchMode="singleTask"/>
        <activity android:name=".Main.AgreementActivity" />
        <activity android:name=".Main.AboutUsActivity" />
        <activity android:name=".Main.PlaceActivity" />
        <activity android:name=".Main.MainPage" />
        <activity android:name=".WelcomePage.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login.RegisterActivity" />
        <activity android:name=".Login.LoginActivity" />
    </application>

</manifest>